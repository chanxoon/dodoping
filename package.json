{
  "name": "portfolio",
  "version": "1.0.0",
  "scripts": {
    "start": "live-server .",
    "scss:watch": "sass --watch assets/scss:assets/css",
    "dev": "npm-run-all --parallel scss:watch start",
    "scss:build": "sass assets/scss/style.scss assets/css/style.css --style=compressed",
    "build": "npm run scss:build"
  },
  "devDependencies": {
    "live-server": "^1.2.2",
    "npm-run-all": "^4.1.5",
    "sass": "^1.69.0"
  }
}
